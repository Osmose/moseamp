{
  "name": "moseamp",
  "productName": "MoseAmp",
  "version": "6.1.0",
  "description": "Music n' stuff.",
  "repository": "Osmose/moseamp",
  "main": "./build/main.js",
  "private": true,
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "12.0.9"
  },
  "scripts": {
    "run": "electron .",
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "compile": "cmake-js build --target musicplayer_node --out native_build",
    "clean": "cmake-js clean --target musicplayer_node --out native_build",
    "start": "concurrently --kill-others --success first \"npm:run\" \"npm:watch\"",
    "package:macos": "bin/package_macos.sh",
    "package:windows": "bash bin\\package_windows.sh",
    "rimraf": "./node_modules/rimraf/bin.js"
  },
  "author": "Michael Kelly",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.14.0",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@reduxjs/toolkit": "^1.5.1",
    "autobind-decorator": "^2.4.0",
    "ccapture.js-npmfixed": "^1.1.0",
    "custom-electron-titlebar": "^4.1.0",
    "electron": "^19.0.0",
    "electron-store": "^8.0.2",
    "electron-window-state": "^5.0.3",
    "lodash": "^4.17.15",
    "m3u-file-parser": "^0.0.9",
    "mousetrap": "1.6.3",
    "music-metadata": "^6.3.0",
    "node-abi": "^2.30.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^12.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0",
    "tmp": "^0.1.0",
    "uuid": "^3.3.3",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-do-expressions": "^7.14.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.12.0",
    "bestzip": "^2.2.1",
    "cmake-js": "^7.2.1",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "electron-packager": "^15.5.1",
    "electron-rebuild": "^3.2.8",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "nan": "^2.14.2",
    "node-addon-api": "^1.7.2",
    "prettier": "^2.3.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "optionalDependencies": {
    "appdmg": "^0.6.0"
  }
}
