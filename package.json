{
  "name": "moseamp",
  "productName": "MoseAmp",
  "version": "5.7.0",
  "description": "Music n' stuff.",
  "repository": "Osmose/moseamp",
  "main": "./build/main.js",
  "private": true,
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "7.1.7"
  },
  "scripts": {
    "run": "electron .",
    "build": "webpack --config webpack.config.js",
    "build:prod": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.config.js",
    "compile": "cmake-js build --target musicplayer_node --out native_build",
    "clean": "cmake-js clean --target musicplayer_node --out native_build",
    "start": "concurrently --kill-others --success first \"npm:run\" \"npm:watch\"",
    "package:macos": "bin/package_macos.sh",
    "package:windows": "bash bin\\package_windows.sh"
  },
  "author": "Michael Kelly",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "autobind-decorator": "^2.4.0",
    "bindings": "^1.5.0",
    "custom-electron-titlebar": "^3.2.1",
    "electron": "^7.1.7",
    "electron-store": "^5.1.0",
    "electron-window-state": "^5.0.3",
    "lodash": "^4.17.15",
    "mousetrap": "1.6.3",
    "music-metadata": "^6.3.0",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bestzip": "^2.1.5",
    "cmake-js": "^6.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "electron-packager": "^14.1.1",
    "electron-rebuild": "^1.8.8",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "nan": "^2.14.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "optionalDependencies": {
    "appdmg": "^0.6.0"
  }
}
